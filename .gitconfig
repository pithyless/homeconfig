[core]
	excludesfile = /Users/norbert/.gitignore_global
[color]
	interactive = true
	status = true
	branch = true
	diff = true
[alias]
	st = status
	ci = commit
	cc = commit
	co = checkout
	br = branch
	s  = status
	a  = add
	d  = diff
	dc = diff --cached
	v  = show
	l  = log --pretty=oneline
	ll = log
	cp = cherry-pick
	;; ri = rebase -i HEAD~10
	;; sh = !git-sh
	;; ro = !git fetch origin && git reset --hard origin/master
